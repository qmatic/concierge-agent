{
  "name": "connect-concierge",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start": "npm run ng-config-dev && ng serve --proxy-config proxy.config.json",
    "build": "ng build --prod",
    "wcag-validate": "cypress run --spec 'cypress/integration/wcag-validation/**/*'",
    "test": "ng test",
    "build-war-dev": "npm run ng-config-dev && ng build --base-href /connectconcierge/ && gulp build:war:properties",
    "build-war-prod": "ng build --prod --base-href /connectconcierge/ && gulp build:war:properties",
    "build-artifactory": "npm run ng-config-prod && ng build --prod --base-href /connectconcierge/ && gulp build:artifactory",
    "deploy-war-dev": "npm run ng-config-dev && ng build && gulp build:war:deploy",
    "deploy-war-prod": "npm run ng-config-prod && ng build --prod --base-href /connectconcierge/ && gulp build:war:deploy",
    "deploy-lang": "gulp deploy:lang",
    "deploy-artifactory": "npm run ng-config-prod && ng build --prod --base-href /connectconcierge/ && gulp build:artifactory:deploy",
    "ng-config-dev": "node build-configuration/build-config.js dev",
    "ng-config-prod": "node build-configuration/build-config.js prod",
    "ntlm-proxy": "start /min \"ntlm-proxy\" cmd /c node_modules\\.bin\\ntlm-proxy",
    "cypress-ntlm": "npm run ntlm-proxy && (cypress-ntlm open & ntlm-proxy-exit)"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.4",
    "@angular/cdk": "^7.0.1",
    "@angular/common": "^7.2.4",
    "@angular/compiler": "^7.2.4",
    "@angular/core": "^7.2.4",
    "@angular/forms": "^7.2.4",
    "@angular/http": "^7.2.4",
    "@angular/platform-browser": "^7.2.4",
    "@angular/platform-browser-dynamic": "^7.2.4",
    "@angular/platform-server": "^7.2.4",
    "@angular/router": "^7.2.4",
    "@ng-bootstrap/ng-bootstrap": "^4.0.2",
    "@ng-select/ng-select": "^2.10.5",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^6.1.2",
    "@ngrx/store": "^6.1.2",
    "@ngrx/store-devtools": "^6.1.2",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "angular-moment-timezone": "^1.7.1",
    "angular2-draggable": "^2.1.8",
    "angular2-moment": "^1.9.0",
    "animate.css": "^3.6.1",
    "bootstrap": "^4.3.1",
    "cometd": "^3.1.4",
    "compressible": "^2.0.14",
    "core-js": "^2.6.4",
    "css-vars-ponyfill": "^1.15.1",
    "hammerjs": "^2.0.8",
    "jquery": "^3.4.1",
    "natives": "^1.1.6",
    "ngx-toastr": "^8.8.0",
    "on-headers": "^1.0.1",
    "rxjs": "^6.2.1",
    "underscore": "^1.9.1",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.9",
    "@angular/cli": "^7.3.9",
    "@angular/compiler-cli": "^7.2.15",
    "@angular/language-service": "^7.2.4",
    "@types/jasmine": "^3.3.8",
    "@types/jasminewd2": "^2.0.6",
    "@types/lodash": "^4.14.117",
    "@types/node": "^11.9.0",
    "axe": "^4.0.4",
    "axe-core": "^3.3.2",
    "codelyzer": "^4.5.0",
    "concurrently": "^4.1.2",
    "cypress": "^3.4.1",
    "cypress-ntlm-auth": "^1.3.1",
    "gulp": "^4.0.2",
    "gulp-sftp": "^0.1.5",
    "gulp-sync": "^0.1.4",
    "gulp-zip": "^4.1.0",
    "jasmine-core": "^3.3.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^2.0.0",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "node-cmd": "^3.0.0",
    "protractor": "^5.4.2",
    "ts-node": "~5.0.1",
    "tslint": "^5.12.1",
    "typescript": "3.2.4"
  }
}
